############################
# Ćwiczenie 2 – „BWM” bez pakietu
############################
# wagi „ostre” zgodne z ekspertem:
# C2 (Wsparcie) – najlepsze, C3 (Łatwość) – najgorsze
crisp_w <- c(
C1_Koszt   = 0.25,
C2_Wsparcie = 0.50,
C3_Latwosc  = 0.25
)
# fuzzyfikacja (l = m = u)
w_C1_bwm <- c(crisp_w["C1_Koszt"],    crisp_w["C1_Koszt"],    crisp_w["C1_Koszt"])
w_C2_bwm <- c(crisp_w["C2_Wsparcie"], crisp_w["C2_Wsparcie"], crisp_w["C2_Wsparcie"])
w_C3_bwm <- c(crisp_w["C3_Latwosc"],  crisp_w["C3_Latwosc"],  crisp_w["C3_Latwosc"])
weights_vector_bwm <- c(w_C1_bwm, w_C2_bwm, w_C3_bwm)
print("Nowy wektor wag (pseudo-BWM):")
print(weights_vector_bwm)
