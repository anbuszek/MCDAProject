# --- Definicje TFN (skala lingwistyczna) ---
BN <- c(1, 1, 3)
N  <- c(1, 3, 5)
S  <- c(3, 5, 7)
W  <- c(5, 7, 9)
BW <- c(7, 9, 9)
# --- Macierz decyzyjna: 3 alternatywy x 3 kryteria (TFN) ---
#        C1(l,m,u), C2(l,m,u), C3(l,m,u)
A1 <- c(W,  S,  N)    # Alpha
A2 <- c(N,  W,  W)    # Beta
A3 <- c(S,  S,  BW)   # Gamma
decision_matrix <- rbind(A1, A2, A3)
# --- Wektor orientacji kryteriów ---
cb_vector <- c("min", "max", "max")
# --- Wektor wag (równe, rozmyte 1/3) ---
w_C1 <- c(1/3, 1/3, 1/3)
w_C2 <- c(1/3, 1/3, 1/3)
w_C3 <- c(1/3, 1/3, 1/3)
weights_vector <- c(w_C1, w_C2, w_C3)
# sprawdź, co jest w środowisku
ls()
# kształt macierzy decyzyjnej: powinno wyjść 3 wiersze, 9 kolumn
dim(decision_matrix)
# podgląd macierzy
decision_matrix
# wektor orientacji
cb_vector
# długość i zawartość wektora wag (powinno być 9)
length(weights_vector)
weights_vector
